<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.loki.server.dao.TradePaycodeDao">
  <cache eviction="LRU" type="com.loki.server.cache.RedisCache"/>
  <resultMap id="tradePaycodeDao" type="com.loki.server.model.TradePaycode">
    <id column="id" jdbcType="INTEGER" property="id" />
    <result column="trade_id" jdbcType="INTEGER" property="tradeId" />
    <result column="paycode_id" jdbcType="INTEGER" property="paycodeId" />
    <result column="paycode_name" jdbcType="VARCHAR" property="paycodeName" />
  </resultMap>
  
  <sql id="Base_Column_List">
    id,trade_id,paycode_id,paycode_name
  </sql>
  
  <insert id="insert" useGeneratedKeys="true" keyProperty="id"  parameterType="com.loki.server.model.TradePaycode">
		insert into trade_paycode(trade_id,paycode_id,paycode_name) 
		values(#{tradeId},#{paycodeId},#{paycodeName})
	</insert>

  <update id="update" parameterType="com.loki.server.model.TradePaycode">
  	update trade_paycode SET trade_id=#{tradeId},paycode_id=#{paycodeId},paycode_name=#{paycodeName} where id=#{id}
  </update>
  
  <select id="findById" parameterType="int" resultMap="tradePaycodeDao">
		select <include refid="Base_Column_List" /> 
		from trade_paycode
		where id=#{_parameter}
  </select>
  
  <select id="findAll" resultMap="tradePaycodeDao">
		select <include refid="Base_Column_List" /> 
		from trade_paycode
  </select>
  
  <delete id="delete" parameterType="int">
		delete from trade_paycode where id=#{_parameter}
	</delete>
</mapper>